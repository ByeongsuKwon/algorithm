#include <stdio.h>
#define ORDER 3

class Btree
{
  BtreeNode* root;

  void insert(BtreeNode* newNode)
  {
    if (root ==0)
    {
      root = newNode;
      return;
    }

  }
}

class BtreeNode
{
  int key;
}

class BtreeDNode : BtreeNode
{
}

class BtreeINode : BtreeNode
{
}

int main()
{
  Btree* btree = new Btree();
  BtreeDNode* dNode = new BtreeDNode();
  btree->insert();


}

